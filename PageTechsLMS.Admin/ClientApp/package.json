{
  "name": "pagetechs-admin-portal",
  "version": "0.1.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "build-prod": "cross-env UMI_ENV=prod umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@alitajs/routes": "^2.8.1",
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.16.6",
    "@ant-design/pro-table": "^2.34.4",
    "@types/auth0-js": "^9.14.4",
    "ace-builds": "^1.4.12",
    "antd": "^4.15.2",
    "auth0-js": "^9.15.0",
    "axios": "^0.19.0",
    "braft-editor": "^2.3.9",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "oidc-client": "^1.11.5",
    "omit.js": "^2.0.2",
    "path-to-regexp": "2.4.0",
    "qiniu-js": "^3.1.3",
    "qs": "^6.10.1",
    "rc-util": "^5.9.9",
    "react": "^16.14.0",
    "react-ace": "^9.4.0",
    "react-contexify": "^4.1.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.14.0",
    "react-helmet-async": "^1.0.9",
    "react-hotkeys": "^2.0.0",
    "reflv": "0.0.1",
    "umi": "^3.2.19",
    "umi-request": "^1.0.8",
    "use-merge-value": "^1.0.1",
    "video-react": "^0.14.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.4",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/qs": "^6.9.6",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-helmet": "^5.0.13",
    "@umijs/fabric": "^2.5.7",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-react": "^1.8.6",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "esbuild-webpack-plugin": "^1.1.0",
    "eslint": "^7.24.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "husky": "^4.3.8",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.2.1",
    "pro-download": "1.0.1",
    "puppeteer-core": "^5.5.0",
    "stylelint": "^13.12.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
