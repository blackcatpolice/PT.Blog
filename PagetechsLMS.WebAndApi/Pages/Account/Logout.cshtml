@page
@model PagetechsLMS.WebAndApi.Pages.Account.LogoutModel
@{
}

<!DOCTYPE html>
<html lang="en"
      dir="ltr">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible"
          content="IE=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Login</title>

    <!-- Prevent the demo from appearing in search engines -->
    <meta name="robots"
          content="noindex">

    <!-- Preloader -->
    <link type="text/css"
          href="/luma/vendor/spinkit.css"
          rel="stylesheet">

    <!-- Perfect Scrollbar -->
    <link type="text/css"
          href="/luma/vendor/perfect-scrollbar.css"
          rel="stylesheet">

    <!-- Material Design Icons -->
    <link type="text/css"
          href="/luma/css/material-icons.css"
          rel="stylesheet">

    <!-- Font Awesome Icons -->
    <link type="text/css"
          href="/luma/css/fontawesome.css"
          rel="stylesheet">

    <!-- Preloader -->
    <link type="text/css"
          href="/luma/css/preloader.css"
          rel="stylesheet">

    <!-- App CSS -->
    <link type="text/css"
          href="/luma/css/app.css"
          rel="stylesheet">

</head>

<body class="layout-default layout-login-centered-boxed">

    <div class="layout-login-centered-boxed__form card" style="margin: auto;">
        <div class="d-flex flex-column justify-content-center align-items-center mt-2 mb-5 navbar-light">
            <a href="/"
               class="navbar-brand flex-column mb-2 align-items-center mr-0"
               style="min-width: 0">

                <span class="avatar avatar-sm navbar-brand-icon mr-0">

                    <span class="avatar-title rounded bg-primary">
                        <img src="/luma/images/illustration/student/128/white.svg"
                             alt="logo"
                             class="img-fluid" />
                    </span>

                </span>

                PTLMS
            </a>
            <p class="m-0">Logout Success ! </p>
        </div> 
         
    </div>

    <!-- jQuery -->
    <script src="/luma/vendor/jquery.min.js"></script>

    <!-- Bootstrap -->
    <script src="/luma/vendor/popper.min.js"></script>
    <script src="/luma/vendor/bootstrap.min.js"></script>

    <!-- Perfect Scrollbar -->
    <script src="/luma/vendor/perfect-scrollbar.min.js"></script>

    <!-- DOM Factory -->
    <script src="/luma/vendor/dom-factory.js"></script>

    <!-- MDK -->
    <script src="/luma/vendor/material-design-kit.js"></script>

    <!-- App JS -->
    <script src="/luma/js/app.js"></script>

    <!-- Preloader -->
    <script src="/luma/js/preloader.js"></script>

</body>

</html>

