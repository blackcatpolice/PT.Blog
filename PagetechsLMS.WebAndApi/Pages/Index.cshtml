@page
@model IndexModel
@{
    ViewData["Title"] = "首页";
    Layout = "_LayoutBigBanner";
}



<div class="mdk-header-layout__content page-content ">
    <div class="border-bottom-2 py-16pt navbar-light bg-white border-bottom-2">
        <div class="container page__container">
            <div class="row align-items-center">
                <div class="d-flex col-md align-items-center border-bottom border-md-0 mb-16pt mb-md-0 pb-16pt pb-md-0">
                    <div class="rounded-circle bg-primary w-64 h-64 d-inline-flex align-items-center justify-content-center mr-16pt">
                        <i class="material-icons text-white">subscriptions</i>
                    </div>
                    <div class="flex">
                        <div class="card-title mb-4pt">8,000+ 课程</div>
                        <p class="card-subtitle text-70">浏览广泛的聚能.</p>
                    </div>
                </div>
                <div class="d-flex col-md align-items-center border-bottom border-md-0 mb-16pt mb-md-0 pb-16pt pb-md-0">
                    <div class="rounded-circle bg-primary w-64 h-64 d-inline-flex align-items-center justify-content-center mr-16pt">
                        <i class="material-icons text-white">verified_user</i>
                    </div>
                    <div class="flex">
                        <div class="card-title mb-4pt">行业专家保证</div>
                        <p class="card-subtitle text-70">业内优秀的开发人员.</p>
                    </div>
                </div>
                <div class="d-flex col-md align-items-center">
                    <div class="rounded-circle bg-primary w-64 h-64 d-inline-flex align-items-center justify-content-center mr-16pt">
                        <i class="material-icons text-white">update</i>
                    </div>
                    <div class="flex">
                        <div class="card-title mb-4pt">无限制的访问</div>
                        <p class="card-subtitle text-70">解锁然后学习主题.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="page-section border-bottom-2">
        <div class="container page__container">

            <div class="page-separator">
                <div class="page-separator__text">热点文章</div>
            </div>

            <div class="row card-group-row">
                @foreach (var item in Model.CourseList)
                {
                    <div class="col-md-6 col-lg-4 card-group-row__col">

                        <div class="card card--elevated posts-card-popular overlay card-group-row__card">
                            <img src="@item.Cover"
                                 alt=""
                                 class="card-img">
                            <div class="fullbleed bg-primary"
                                 style="opacity: .5"></div>
                            <div class="posts-card-popular__content">
                                <div class="card-body d-flex align-items-center">
                                    <div class="avatar-group flex">
                                        <div class="avatar avatar-xs"
                                             data-toggle="tooltip"
                                             data-placement="top"
                                             title="Janell D.">
                                            <a href="">
                                                <img src="@item.Cover"
                                                     alt="Avatar"
                                                     class="avatar-img rounded-circle">
                                            </a>
                                        </div>
                                    </div>
                                    <a style="text-decoration: none;"
                                       class="d-flex align-items-center"
                                       href="">
                                        <i class="material-icons mr-1"
                                           style="font-size: inherit;">remove_red_eye</i> <small>@item.ViewNumb</small>
                                    </a>
                                </div>
                                <div class="posts-card-popular__title card-body">
                                    <small class="text-muted text-uppercase">@item.Name</small>
                                    <a class="card-title"
                                       href="/Courses/TrailerCourse/?id=@item.Id">@item.Name</a>
                                    @*<a class="card-title"
                                       href="/Courses/CourseDetail/?id=@item.Id">@item.Name</a>*@
                                </div>
                            </div>
                        </div>

                    </div>
                } 

            </div>  
        </div>
    </div>

   

